{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "barryvdh/laravel-ide-helper": "^2.2",
        "erusev/parsedown": "^1.6",
        "spatie/laravel-medialibrary": "^4.9.0",
        "cyvelnet/laravel5-fractal": "^1.3",
        "vinkla/hashids": "^2.3",
        "cviebrock/eloquent-sluggable": "^4.1",
        "hootlex/laravel-moderation": "^1.0",
        "hieu-le/active": "^3.3",
        "predis/predis": "^1.1",
        "doctrine/dbal": "^2.5",
        "laravel/socialite": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "intervention/image": "^2.3",
        "eusonlito/laravel-meta": "^3.0",
        "larapack/config-writer": "1.*",
        "ezyang/htmlpurifier": "^4.8",
        "RobBrazier/Piwik": "~2.1",
        "tamayo/laravel-scout-elastic": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Models\\": "app/Models/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
